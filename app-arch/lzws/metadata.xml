    <flag name="bignum_library_tommath">Use tommath as bignum library</flag>
    <flag name="static">Build static binaries</flag>
    <flag name="test">Build and run tests</flag>
    <flag name="noman">Disable creating of man pages</flag>
  </use>
</pkgmetadata>
